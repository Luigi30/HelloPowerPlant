#include "ShapeHelper.h"Point ShapeHelper::FindCentroid(Rect r){	Point p;		p.v = (r.top + r.bottom) / 2;	p.h = (r.left + r.right) / 2;		return p;}Point ShapeHelper::RotatePointAroundOrigin(Point p, SInt16 degrees){	Point rotated;		float radians = degrees * (M_PI / 180.0);	rotated.h = (p.h * std::cos(radians)) - (p.v * std::sin(radians));	rotated.v = (p.v * std::cos(radians)) + (p.h * std::sin(radians));		return rotated;}